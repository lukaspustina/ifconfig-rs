                                                        <li>
                                                            <dl class="uk-description-list">
                                                                <dt>JSON</dt>
                                                                <script>
                                                                    $(document).ready(get_ifconfig_json("/isp", "#api-json-isp"));
                                                                </script>
                                                                <dd><pre id="api-json-isp" class="mono-pre"></pre></dd>
                                                            </dl>
                                                        </li>
                                                    </ul>
                                                </li>
                                                <li>
                                                    <ul class="uk-list uk-list-bullet">
                                                        <li>
                                                            <dl class="uk-description-list">
                                                                <dt>Plain</dt>
                                                                <dd>
                                                                    <pre class="mono-pre">{{ ifconfig.user_agent.name }}, {{ ifconfig.user_agent.version }}, {{ ifconfig.user_agent.os }}, {{ ifconfig.user_agent.os_version }} </pre>
                                                                </dd>
